    "@kuzzleio/iot-platform-frontend": "3.5.0",
    "@vuelidate/core": "2.0.*",
    "kuzzle-device-manager-types": "2.7.2",
    "lodash": "4.17.21",
    "vue-plugin-kuzzle": "4.6.0"
  },
  "devDependencies": {
    "@types/lodash": "4.17",
    "@vitejs/plugin-vue2": "2.3.3",
    "autoprefixer": "10.4.20",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-vue-kuzzle": "0.0.12",
    "sass": "1.83.*",
    "tailwindcss": "3.4.17",
    "typescript": "5.4.*",
    "vite": "6.0.7",
    "vue-tsc": "2.2.0"
