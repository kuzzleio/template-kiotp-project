  },
  "devDependencies": {
    "@types/lodash": "4.17",
    "@vitejs/plugin-vue2": "2.3.4",
    "autoprefixer": "10.4.21",
    "eslint-plugin-vue-kuzzle": "0.0.14",
    "sass": "1.93.*",
    "tailwindcss": "3.4.17",
    "typescript": "5.4.*",
    "vite": "6.3.5",
    "vue-tsc": "2.2.10",
    "jest": "30.2.0"
